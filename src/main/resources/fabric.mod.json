{
  "schemaVersion": 1,
  "id": "dinocraft",
  "version": "${version}",

  "name": "Dinocraft",
  "description": "Dinocraft is a mod that contains new dinosaur and prehistoric items, blocks and mobs to the game.",
  "authors": [
    "Bonkersboy2017",
    "Liukrast",
    "dejoVSscimmia",
    "Treetrain1"
  ],
  "contact": {
    "homepage": " http://thedinocrew.website2.me/",
    "sources": "https://github.com/Bonkersboy2017/DinocraftMod",
    "issues": "https://github.com/Bonkersboy2017/DinocraftMod/issues"
  },

  "license": "cc",
  "icon": "assets/dinocraft/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.dinocrew.dinocraft.Dinocraft"
    ],
    "client": [
      "com.dinocrew.dinocraft.DinocraftClient"
    ],
    "fabric-datagen": [
      "com.dinocrew.dinocraft.misc.DinocraftDataGenerator"
    ],
    "frozenlib:main": [
      "com.dinocrew.dinocraft.misc.FrozenLibIntegration"
    ],
    "frozenlib:events": [
      "com.dinocrew.dinocraft.registry.RegisterWorldgen"
    ],
    "modmenu": [
      "com.dinocrew.dinocraft.misc.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "dinocraft.mixins.json"
  ],

  "accessWidener": "dinocraft.accesswidener",

  "depends": {
    "fabricloader": ">=0.14.11",
    "fabric-api": ">=0.69.0",
    "minecraft": ">=1.19.3",
    "java": ">=17"
  }
}
